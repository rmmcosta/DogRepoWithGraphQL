type Dog {
    id: ID!,
    name: String!,
    breed: String!,
    origin: String!
}

type Query {
    findAllDogs(count: Int, offset: Int):[Dog]!
    findDogById(id:ID!):Dog!
}

type Mutation {
    newDog(name: String!, breed: String!, origin:String!):Dog!
    deleteDog(id:ID!):Boolean
    updateDogName(newName: String!, id:ID!):Dog!
}